  <script>
    // –§—É–Ω–∫—Ü–∏—è –¥–ª—è –≥–µ–Ω–µ—Ä–∞—Ü–∏–∏ —Å–ª—É—á–∞–π–Ω–æ–≥–æ —á–∏—Å–ª–∞ –≤ –∑–∞–¥–∞–Ω–Ω–æ–º –¥–∏–∞–ø–∞–∑–æ–Ω–µ
    function getRandomNumber(min, max) {
      return Math.random() * (max - min) + min;
    }

    // –§—É–Ω–∫—Ü–∏—è –¥–ª—è –≥–µ–Ω–µ—Ä–∞—Ü–∏–∏ —Å–ª—É—á–∞–π–Ω–æ–≥–æ —Ü–≤–µ—Ç–∞
    function getRandomColor() {
      const letters = '0123456789ABCDEF';
      let color = '#';
      for (let i = 0; i < 6; i++) {
        color += letters[Math.floor(Math.random() * 16)];
      }
      return color;
    }

    // –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Å–æ–∑–¥–∞–Ω–∏—è –ª–µ—Ç–∞—é—â–µ–π —Ü–∏—Ñ—Ä—ã
    function createFlyingDigit() {
      const digit = document.createElement('div');
      digit.classList.add('digit');

      // –£—Å—Ç–∞–Ω–æ–≤–∫–∞ —Å–ª—É—á–∞–π–Ω–æ–π —Ü–∏—Ñ—Ä—ã –æ—Ç 0 –¥–æ 23
      digit.textContent = Math.floor(getRandomNumber(0, 23));

      // –£—Å—Ç–∞–Ω–æ–≤–∫–∞ —Å–ª—É—á–∞–π–Ω–æ–≥–æ —Ü–≤–µ—Ç–∞
      digit.style.color = getRandomColor();

      // –£—Å—Ç–∞–Ω–æ–≤–∫–∞ —Å–ª—É—á–∞–π–Ω–æ–π –Ω–∞—á–∞–ª—å–Ω–æ–π –ø–æ–∑–∏—Ü–∏–∏
      digit.style.left = `${getRandomNumber(0, window.innerWidth)}px`;
      digit.style.top = `${getRandomNumber(0, window.innerHeight)}px`;

      // –£—Å—Ç–∞–Ω–æ–≤–∫–∞ —Å–ª—É—á–∞–π–Ω–æ–≥–æ –º–∞—Å—à—Ç–∞–±–∞ (—Ä–∞–∑–º–µ—Ä–∞)
      const scale = getRandomNumber(0.5, 2); // –ú–∞—Å—à—Ç–∞–± –æ—Ç 0.5 –¥–æ 2
      digit.style.transform = `scale(${scale})`;

      // –£—Å—Ç–∞–Ω–æ–≤–∫–∞ —Å–ª—É—á–∞–π–Ω–æ–π –¥–ª–∏—Ç–µ–ª—å–Ω–æ—Å—Ç–∏ –∞–Ω–∏–º–∞—Ü–∏–∏
      digit.style.animationDuration = `${getRandomNumber(8, 15)}s`;

      // –î–æ–±–∞–≤–ª–µ–Ω–∏–µ —Ü–∏—Ñ—Ä—ã –Ω–∞ —Å—Ç—Ä–∞–Ω–∏—Ü—É
      document.body.appendChild(digit);

      // –£–¥–∞–ª–µ–Ω–∏–µ —ç–ª–µ–º–µ–Ω—Ç–∞ –ø–æ—Å–ª–µ –∑–∞–≤–µ—Ä—à–µ–Ω–∏—è –∞–Ω–∏–º–∞—Ü–∏–∏
      digit.addEventListener('animationend', () => {
        digit.remove();
      });
    }

    // –°–æ–∑–¥–∞–Ω–∏–µ –Ω–æ–≤—ã—Ö —Ü–∏—Ñ—Ä –∫–∞–∂–¥—ã–µ 500 –º—Å
    setInterval(createFlyingDigit, 500);
  </script>

